<p id="notice"><%= notice %></p>

<h1>Liste des catégories</h1>

<%= paginate @categories %>

<table class="table">
  <thead>
    <tr>
      <th>ID boutique</th>
      <th>Libellé</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.private_id %></td>
        <td><%= category.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
